{
  "name": "@bitdrift/webview-sdk",
  "version": "1.0.0",
  "private": true,
  "description": "Bitdrift WebView JavaScript SDK for capturing performance metrics and network events",
  "main": "dist/bitdrift-webview.js",
  "scripts": {
    "build": "tsx scripts/build.ts",
    "watch": "tsx scripts/build.ts --watch",
    "generate": "tsx scripts/generate-native.ts",
    "format": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/node": "^24.10.8",
    "esbuild": "^0.27.2",
    "jsdom": "^26.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "web-vitals": "^5.1.0"
  }
}
